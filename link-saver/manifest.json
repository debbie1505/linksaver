{
  "name": "Tab Saver",
  "description": "A Browser Extension that allows you to store your links locally in a save place for future references.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": ""
  }
}

{
  "name": "Link Saver",
  "description": "A Chrome Extension that allows you to store your links locally in a save place for future references.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "Designer.png",
    "default_title": "Save your links"
  },

  "icons": {
    "16": "Designer.png",
    "32": "Designer.png",
    "48": "Designer.png",
    "128": "Designer.png"
  },

  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls"],

  "options_ui": {
    "page": "index.html",
    "open_in_tab": false
  }
}
